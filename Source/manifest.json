{
	"manifest_version": 2,
	"name": "Cuck to Scary Ghost",
	"version": "0.5.2",
	"description": "Replaces the text 'Cuck' with 'Scary Ghost'.",
	"icons": { 
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png" 
	},
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

